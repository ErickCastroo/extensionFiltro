{
  "manifest_version": 3,
  "name": "Buscador de Folios desde Excel",
  "version": "1.0",
  "description": "Carga un archivo Excel con folios y los resalta en la p√°gina.",
  "permissions": ["activeTab", "scripting"],
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "images/logooomapasgris.png",
      "48": "images/logooomapasgris.png",
      "128": "images/logooomapasgris.png"
    }
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["content.js"]
    }
  ]
}